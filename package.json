{
  "name": "modular-boilerplate",
  "version": "1.0.0",
  "description": "modular boilerplate node express",
  "main": "app.js",
  "scripts": {
    "postinstall": "link-module-alias",
    "start": "NODE_ENV=production exec node app.js",
    "dev": "NODE_ENV=development exec nodemon app.js"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "lijomon c john",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.15"
  },
  "_moduleAliases": {
    "root": ".",
    "auth": "./modules/auth",
    "user": "./modules/user",
    "utilities": "./modules/utils",
    "consts": "./modules/constants"
  }
}
